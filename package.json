{
  "name": "@guildeducationinc/employer-portal-remote-template",
  "author": "Guild Education Inc",
  "version": "1.0.0",
  "license": "LicenseRef-LICENSE",
  "description": "Employer Portal MFE Remote Template",
  "private": true,
  "engines": {
    "node": ">=18",
    "yarn": ">=1.22"
  },
  "scripts": {
    "setup": "scripts/setup.sh",
    "build": "cross-env NODE_ENV=production webpack",
    "build:cypress": "cross-env NODE_ENV=production REACT_APP_ENABLE_CYPRESS=true webpack",
    "build:report": "cross-env NODE_ENV=production WEBPACK_REPORT=true webpack",
    "clean": "rimraf coverage dist public *.log",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "format": "prettier --write \"src/**/*.{ts,tsx,scss,json,js,jsx,css}\"",
    "format:check": "prettier --list-different \"src/**/*.{ts,tsx,scss,json,js,jsx,css}\"",
    "lint": "yarn lint:script && yarn lint:style",
    "lint:fix": "yarn lint:script --fix && yarn lint:style --fix",
    "lint:script": "eslint . --ext js,jsx,ts,tsx",
    "lint:style": "stylelint src",
    "prepare": "husky install",
    "start": "scripts/start.sh",
    "start:ws": "ws -p 8080 -d public --spa index.html",
    "test": "cross-env NODE_ENV=test jest --maxWorkers=2",
    "test:coverage": "jest --coverage --colors --maxWorkers=2",
    "test:coverage:open": "yarn test:coverage; open coverage/lcov-report/index.html",
    "test:e2e": "yarn start & wait-on http://localhost:8080 & cypress run",
    "test:watch": "yarn jest --watchAll",
    "typewriter:dev": "npx typewriter development",
    "typewriter:prod": "npx typewriter production"
  },
